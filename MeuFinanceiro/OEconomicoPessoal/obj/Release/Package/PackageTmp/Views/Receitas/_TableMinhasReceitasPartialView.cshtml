@model  OEconomicoPessoal.Models.ViewModel.ReceitasViewModel

@if (Model != null && Model.GetReceitas.Count > 0)
        {
<!-- Example DataTables Card-->
<div class="card mb-3">
    <div class="card-header bg-info">
        <i class="fa fa-table"></i> Receitas do mês
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Código</th>
                        <th>Data</th>
                        <th>Valor</th>
                        <th>Descrição</th>
                        <th>Pagamento</th>
                    </tr>
                </thead>
                @if (Model != null)
                {
                    foreach (var item in Model.GetReceitas)
                    {
                        <tbody>
                            <tr>
                                <td id="id">@item.Id</td>
                                <td id="titulo">@item.DataCadastro</td>
                                <td id="descricao">R$ @item.Valor</td>
                                <td id="tipoarquivo">@item.Descricao</td>
                                <td id="nomearquivo">@item.TipoPagamentos.Descricao</td>
                                <td>
                                    <a href="@Url.Action("editarreceita","receitas", new { id = @item.Id })" id="editardespesa" name="@item.Id">
                                        Editar
                                    </a>
                                </td>
                            </tr>

                        </tbody>
                    }
                }
                @*<tfoot>
                        <tr>
                            <th>Name</th>
                            <th>Position</th>
                            <th>Office</th>
                            <th>Age</th>
                            <th>Start date</th>
                            <th>Salary</th>
                        </tr>
                    </tfoot>*@
            </table>
        </div>
    </div>
    <div class="card-footer small text-muted">Última atualização em @DateTime.Now.Hour:@DateTime.Now.Minute</div>
</div>
}